<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFrameworks>net45;netcoreapp1.0;netstandard1.0</TargetFrameworks>
        <PackageId>Stream.ReportingCopyTo</PackageId>
        <Authors>Bruno Zell</Authors>
        <Description>Provides Stream.CopyTo overloads for reporting the current relative or absolute progress while copying.</Description>
        <Copyright>Bruno Zell</Copyright>
        <PackageLicenseUrl>https://github.com/TheInsaneBro/Stream.ReportingCopyTo/blob/master/LICENSE</PackageLicenseUrl>
        <PackageProjectUrl>https://github.com/TheInsaneBro/Stream.ReportingCopyTo</PackageProjectUrl>
        <RepositoryUrl>https://github.com/TheInsaneBro/Stream.ReportingCopyTo</RepositoryUrl>
        <PackageTags>async asynchronous stream copyto reporting task progress iprogress system io</PackageTags>
        <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
        <LangVersion>latest</LangVersion>
        <RootNamespace>System.IO</RootNamespace>
        <Version>2.0.0</Version>
        <Product>Stream.ReportingCopyTo</Product>
    </PropertyGroup>

</Project>
